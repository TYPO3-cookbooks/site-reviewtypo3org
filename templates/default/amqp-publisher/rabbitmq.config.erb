[amqp]
  uri = <%= @amqp[:uri] %>
  username = <%= @amqp[:username] %>
  password = <%= @amqp[:password] %>
[exchange]
  name = org.typo3.review.events
[message]
  routingKey = org.typo3.review.events
[gerrit]
  name = review
  hostname = review.typo3.org
  scheme = ssh
  port = 24918
[monitor]
  interval = 15000
  failureCount = 15